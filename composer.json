{
    "name": "terrazza/annotation",
    "type" : "library",
    "version": "1.0.0",
    "description": "Terrazza Component Annotation",
    "license": "MIT",
    "config" : {
        "vendor-dir" : "plugin"
    },
    "authors": [
        {
            "name": "Peter Fux",
            "email": "it@noframework.io"
        }
    ],
    "require": {
        "php": ">=7.4",
        "terrazza/reflectionclass": "dev-main",
        "psr/log": "1.1.*"
    },
    "require-dev" : {
        "phpunit/phpunit": "^9.3",
        "terrazza/logger": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Terrazza\\Component\\Annotation\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Terrazza\\Component\\Annotation\\Tests\\_Mocks\\" : "tests\\_Mocks",
            "Terrazza\\Component\\Annotation\\Tests\\_Examples\\" : "tests\\_Examples"
        }
    },
    "minimum-stability": "dev"
}